doctype html
html(lang="en")
  head
    title= title
    script(src='//ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js')
    script(src='/front-end.js')
    link(rel='stylesheet', type='text/css', href='/styles.css')
  body
    h4#title-id= title
    p= message
    
    table
      thead
        th id
        th board string
      tbody
        each game in games
          tr
            td= game.id
            td= game.board
